{
  "expo": {
    "name": "Run Shift",
    "slug": "run-shift",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconOne.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#175B57"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-iconOne.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thetrueseeker.runshift",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-router",
      "expo-image-picker",
      "expo-font",
      "expo-local-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "31611472-44c3-460c-9ac9-54249dd2780b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/31611472-44c3-460c-9ac9-54249dd2780b"
    }
  }
}
